    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "style-loader": "0.23.0",
    "ts-loader": "9.4.2",
    "tslib": "2.4.1",
    "typescript": "^4.9.4",
    "url-loader": "4.1.1",
    "webpack": "~5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "~4.8.1"
  },
