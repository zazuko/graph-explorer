{
  "name": "graph-explorer",
  "version": "0.12.0",
  "description": "Graph Explorer can be used to explore and RDF graphs in SPARQL endpoints or on the web.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rdf-tools/graph-explorer.git"
  },
  "keywords": [
    "owl",
    "graph",
    "rdf",
    "diagram",
    "sparql"
  ],
  "author": {
    "name": "RDF Tools",
    "url": "https://github.com/rdf-tools/graph-explorer"
  },
  "license": "LGPL-2.1",
  "bugs": {
    "url": "https://github.com/rdf-tools/graph-explorer/issues"
  },
  "homepage": "https://github.com/rdf-tools/graph-explorer#readme",
  "scripts": {
    "demo": "./node_modules/.bin/webpack-dev-server --port 10444 --config webpack.demo.config.js",
    "build": "npm run _webpack && npm run typings",
    "build-all": "npm run _webpack && BUNDLE_PEERS=true npm run _webpack",
    "build-examples": "./node_modules/.bin/webpack --config webpack.demo.config.js",
    "test": "npm run build-all && npm run lint",
    "typings": "npm run _typings-tsc && npm run _typings-dts-bundle",
    "lint": "eslint './{src,examples}/**/*.{ts,tsx}'",
    "_typings-tsc": "./node_modules/.bin/tsc --project tsconfig.typings.json",
    "_typings-dts-bundle": "./node_modules/.bin/dts-bundle --configJson dts-bundle.json",
    "_webpack": "./node_modules/.bin/webpack"
  },
  "dependencies": {
    "d3-color": "^1.4.1",
    "file-saverjs": "~1.3.6",
    "lodash": "~4.17.10",
    "n3": "~0.10.0",
    "rdf-ext": "~0.3.0",
    "webcola": "^3.4.0",
    "whatwg-fetch": "~2.0.2"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@types/d3-color": "^1.2.2",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/react-dom": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "canvg-fixed": "1.0.0",
    "css-loader": "^3.4.0",
    "dts-bundle": "0.7.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.2",
    "raw-loader": "0.5.1",
    "rdf-parser-jsonld": "0.3.3",
    "rdf-parser-n3": "0.3.0",
    "rdf-parser-rdfxml": "0.3.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "ts-loader": "6.2.1",
    "tslib": "1.10.0",
    "typescript": "^3.9.7",
    "url-loader": "1.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "resolutions": {
    "@types/react": "15.6.9"
  },
  "main": "dist/graph-explorer.js",
  "typings": "dist/graph-explorer.d.ts"
}
